{
  "expo": {
    "name": "napp",
    "slug": "napp",
    "version": "1.0.0",
    "scheme": "napp",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": true,
          "supportsPictureInPicture": true
        }
      ],
      "expo-font"
    ],
    "orientation": "portrait",
    "icon": "./assets/solar-energy.gif",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.erenyeager2000.napp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.erenyeager2000.napp"
    },
    "extra": {
      "eas": {
        "projectId": "b6e96696-f8ab-4f8e-aacc-0a7d65f29cf9"
      }
    }
  }
}
